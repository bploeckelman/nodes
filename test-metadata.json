{
  assetTypes: [
    {
      id: image
      name: Image
      basePath: ./sprites
      items: [
        {id: hero-portrait,    name: Hero Portrait,    path: characters/man-idle.png}
        {id: hero-idle,        name: Hero Idle,        path: characters/man-rope.png}
        {id: hero-attack,      name: Hero Attack,      path: characters/man-think.png}
        {id: villain-portrait, name: Villain Portrait, path: characters/man-shove.png}
        {id: villain-idle,     name: Villain Idle,     path: characters/man-down.png}
        {id: villain-attack,   name: Villain Attack,   path: characters/man-hit.png}
      ]
    }
    {
      id: character
      name: Character
      items: [
        {
          id: hero
          name: Hero
          properties: {
            portrait: {type: image, id: hero-portrait, class: net.bplo.nodes.editor.meta.Metadata$AssetItemRef}
            poses: [
              {type: image, id: hero-idle, class: net.bplo.nodes.editor.meta.Metadata$AssetItemRef}
              {type: image, id: hero-attack, class: net.bplo.nodes.editor.meta.Metadata$AssetItemRef}
            ]
          }
        }
        {
          id: villain
          name: Villain
          properties: {
            portrait: {type: image, id: villain-portrait, class: net.bplo.nodes.editor.meta.Metadata$AssetItemRef}
            poses: [
              {type: image, id: villain-idle, class: net.bplo.nodes.editor.meta.Metadata$AssetItemRef}
              {type: image, id: villain-attack, class: net.bplo.nodes.editor.meta.Metadata$AssetItemRef}
            ]
          }
        }
      ]
    }
  ]
  nodeTypes: [
    {
      id: test
      name: Test
      inputs: 1
      outputs: 1
      props: [
        {
          type: select
          id: character-select
          name: Character
          assetType: character
          display: "#{value}.name"
        }
        {
          type: thumbnail
          id: character-portrait
          name: Portrait
          assetType: character
          display: "#{value}.portrait"
          dependsOn: character-select
        }
        {
          type: select
          id: character-pose-select
          name: Pose
          assetType: character
          display: "#{value}.poses"
          dependsOn: character-select
        }
        {
          type: thumbnail
          id: character-pose-thumb
          name: Portrait
          assetType: character
          display: "#{value}.poses"
          dependsOn: character-pose-select
        }
        {
          type: input-text
          id: character-name-override
          name: Override Name
        }
        {
          type: integer
          id: integer
          name: Number
        }
        {
          type: float
          id: float
          name: Number
        }
        {
          type: input-text-multiline
          id: dialogue-text
          name: Text
        }
      ]
    }
  ]
}
